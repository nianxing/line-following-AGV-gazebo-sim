<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="agv">  

    <link name="base">

      <inertial>
        <pose>0.0 0 0.0 0 0 0</pose>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.001</iyy>
          <iyz>0.000</iyz>
          <izz>0.001</izz>
        </inertia>
        <mass>100.0</mass>
      </inertial>

      <collision name="base_collision">
        <pose>0.0 0 0.120 0 0 0</pose>
        <geometry>
          <box>
            <size>1.0 0.8 0.240</size>
          </box>
        </geometry>
      </collision>

      <visual name="base_visual">
        <pose>0.0 0 0.05 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://agv_common_meshes/agv.dae</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="left_wheel">
      <inertial>
        <pose>0.0 -0.35 0.18 0 0 0</pose>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.001</iyy>
          <iyz>0.000</iyz>
          <izz>0.001</izz>
        </inertia>
        <mass>15</mass>
      </inertial>

      <collision name="left_wheel_collision">
        <pose>0.0 -0.35 0.18 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.18</radius>
            <length>0.22</length>
          </cylinder>
        </geometry>
          <surface>
          <!-- This friction pamareter don't contain reliable data!! -->
            <friction>
              <ode>
                <mu>100000.0</mu>
                <mu2>100000.0</mu2>
                <fdir1>0 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
          </friction>
        </surface>
      </collision>

      <visual name="left_wheel_visual">
        <pose>0.0 -0.35 0.18 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://agv_common_meshes/wheel.dae</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="right_wheel">
      <inertial>
        <pose>0.0 0.35 0.18 0 0 0</pose>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.001</iyy>
          <iyz>0.000</iyz>
          <izz>0.001</izz>
        </inertia>
        <mass>1.5</mass>
      </inertial>
    
      <collision name="right_wheel_collision">
        <pose>0.0 0.35 0.18 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.18</radius>
            <length>0.22</length>
          </cylinder>
        </geometry>
          <surface>
          <!-- This friction pamareter don't contain reliable data!! -->
            <friction>
              <ode>
                <mu>100000.0</mu>
                <mu2>100000.0</mu2>
                <fdir1>0 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
          </friction>
        </surface>
      </collision>

      <visual name="right_wheel_visual">
        <pose>0.0 0.35 0.18 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://agv_common_meshes/wheel.dae</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </visual>
    </link>

    <joint name="left_wheel_joint" type="revolute">
      <parent>base</parent>
      <child>left_wheel</child>
      <pose>0.0 -0.35 0.18 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint name="right_wheel_joint" type="revolute">
      <parent>base</parent>
      <child>right_wheel</child>
      <pose>0.0 0.35 0.18 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

  <static>0</static>
  </model>
</sdf>

<?xml version="1.0" ?>
<robot name="agv_for_line_following" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find agv_description)/urdf/common_properties.xacro"/>
  <xacro:include filename="$(find agv_description)/urdf/agv_for_line_following.gazebo.xacro"/>

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.05" rpy="0 0 1.57"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin xyz="0.0 0 0.05" rpy="0 0 1.57"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/bases/agv_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="0.0 0 0.05" rpy="0 0 1.57"/>
      <geometry>
        <box size="1.0 0.8 0.24"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="100"/>
      <inertia ixx="0" ixy="0" ixz="0"
               iyy="0" iyz="0"
               izz="0" />
    </inertial> 
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <origin xyz="0.0 -0.35 0.18" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_left_link">
    <!--visual>
      <origin xyz="0.0 -0.35 0.18" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/wheels/wheel_left.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="white"/>
    </visual-->

    <!--collision>
      <origin xyz="0.0 -0.35 0.18" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.18"/>
      </geometry>
    </collision-->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="15.0" />
      <inertia ixx="0" ixy="0" ixz="0"
               iyy="0" iyz="0"
               izz="0" />
      </inertial>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0.0 0.28 0.16" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_right_link">
    <!--visual>
      <origin xyz="0.0 0.28 0.16" rpy="0 0 3.14"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/wheels/wheel_right.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual-->

    <!--collision>
      <origin xyz="0.0 0.28 0.16" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.18"/>
      </geometry>
    </collision-->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="15.0" />
      <inertia ixx="0" ixy="0" ixz="0"
               iyy="0" iyz="0"
               izz="0" />
      </inertial>
  </link>

  <joint name="caster_left_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_left_link"/>
    <origin xyz="0 0.3118 0.03" rpy="0 0 1.57"/>
  </joint>

  <link name="caster_left_link">
    <!--collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
       <geometry>
        <mesh filename="package://agv_description/meshes/wheels/caster_left.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision-->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="5.0" />
      <inertia ixx="0.00" ixy="0.0" ixz="0.0"
               iyy="0.00" iyz="0.0"
               izz="0.00" />
    </inertial>
  </link>

  <joint name="caster_right_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_right_link"/>
    <origin xyz="0 -0.3118 0.03" rpy="0 0 1.57"/>
  </joint>

  <link name="caster_right_link">
    <!--collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/wheels/caster_right.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision-->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="5.0" />
      <inertia ixx="0.00" ixy="0.0" ixz="0.0"
               iyy="0.00" iyz="0.0"
               izz="0.00" />
    </inertial>
  </link>

</robot>

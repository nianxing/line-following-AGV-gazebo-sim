<?xml version="1.0" ?>
<robot name="agv_for_line_following" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find agv_description)/urdf/common_properties.xacro"/>
  <xacro:include filename="$(find agv_description)/urdf/agv_for_line_following.gazebo.xacro"/>

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.1" rpy="0 0 1.57"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin xyz="0.0 0 0.12" rpy="0 0 1.57"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/bases/agv_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="0.0 0 0.12" rpy="0 0 0"/>
      <geometry>
        <box size="1.0 0.8 0.24"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10"/>
      <inertia ixx="6.48224043" ixy="5.06925791e-2" ixz="2.42332909e-2"
               iyy="2.65332346" iyz="-1.25236226e-2"
               izz="7.7704931" />
    </inertial> 
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <origin xyz="-0.4 0.0 0.075" rpy="1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_left_link">
    <!--visual>
      <origin xyz="0.0 -0.35 0.18" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/wheels/wheel_left.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="white"/>
    </visual-->

    <collision>
      <origin xyz="0.0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.09"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1.5" />
      <inertia ixx="4.05139588e-2" ixy="5.22245694e-3" ixz="1.40418470e-3"
               iyy="4.88302387e-2" iyz="1.86962703e-3"
               izz="4.58877115e-2" />
      </inertial>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0.3 0.0 0.075" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_right_link">
    <!--visual>
      <origin xyz="0.0 0.28 0.16" rpy="0 0 3.14"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/wheels/wheel_right.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual-->

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.09"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1.5" />
      <inertia ixx="4.05139588e-2" ixy="5.22245694e-3" ixz="1.40418470e-3"
               iyy="4.88302387e-2" iyz="1.86962703e-3"
               izz="4.58877115e-2" />
      </inertial>
  </link>

  <joint name="caster_left_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_left_link"/>
    <origin xyz="0 0.3118 0.03" rpy="0 0 1.57"/>
  </joint>

  <link name="caster_left_link">
    <!--collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
       <geometry>
        <mesh filename="package://agv_description/meshes/wheels/caster_left.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision-->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="3.21479325e-03" ixy="-1.62611171e-05" ixz="-7.91864042e-04"
               iyy="4.53599232e-03" iyz="-3.15202900e-05"
               izz="2.24290037e-03" />
      </inertial>
  </link>

  <joint name="caster_right_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_right_link"/>
    <origin xyz="0 -0.3118 0.03" rpy="0 0 1.57"/>
  </joint>

  <link name="caster_right_link">
    <!--collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agv_description/meshes/wheels/caster_right.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision-->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="3.21479325e-03" ixy="-1.62611171e-05" ixz="-7.91864042e-04"
               iyy="4.53599232e-03" iyz="-3.15202900e-05"
               izz="2.24290037e-03" />
      </inertial>
  </link>
</robot>
